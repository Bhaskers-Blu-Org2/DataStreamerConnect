{
    "id": "DataStreamerConnect",
    "name": "Microsoft Data Streamer Connect",
    "title": "Microsoft Data Streamer Connect",
    "corepkg": "core",
    "cloud": {
        "workspace": false,
        "packages": false,
        "sharing": false,
        "publishing": false,
        "githubPackages": false
    },
    "bundleddirs": [
        "libs/core"
    ],
    "runtime": {
      "mathBlocks": true,
      "loopsBlocks": true,
      "logicBlocks": true,
      "variablesBlocks": true,
      "textBlocks": true,
      "functionBlocks": true,
      "listsBlocks": true,
      "onStartColor": "#008272"
    },
    "simulator": {
        "autoRun": true,
        "headless": true,
        "hideFullscreen": false
    },
    "appTheme": {
        "appPathNames": ["/index.html"],
        "logoUrl": "https://github.com/Microsoft/pxt-sample",
        "homeUrl": "http://microsoft.github.io/pxt-sample/",
        "privacyUrl": "https://go.microsoft.com/fwlink/?LinkId=521839",
        "termsOfUseUrl": "https://go.microsoft.com/fwlink/?LinkID=206977",
        "betaUrl": "https://makecode.com/",
        "docMenu": [
            {
                "name": "About",
                "path": "/about"
            }
        ],
        "coloredToolbox": true,
        "monacoToolbox": true,
        "invertedMenu": true,
        "showHomeScreen": true,
        "homeScreenHero": "./static/hero.png",        
        "blocklyOptions": { 
            "grid": {
                "spacing": 45,
                "length": 7,
                "colour": "rgba(189, 195, 199, 0.30)",
                "snap": true
            }
        },
        "availableLocales": [
            "en"
        ],
        "monacoColors": {
            "editor.background": "#ecf0f1"
        }
    }
}
